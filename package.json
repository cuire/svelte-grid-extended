{
	"name": "svelte-grid-extended",
	"version": "1.1.4",
	"description": "A draggable and resizable grid layout, for Svelte",
	"repository": "https://github.com/cuire/svelte-grid-extended",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && yarn package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "yarn package",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"e2e": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "husky install"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"keywords": [
		"svelte",
		"grid",
		"drag",
		"draggable",
		"resize",
		"resizable",
		"fluid",
		"responsive"
	],
	"author": "cuire <garwes@icloud.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/cuire/svelte-grid-extended/issues"
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.41.2",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"@vitest/coverage-c8": "^0.33.0",
		"autoprefixer": "^10.4.12",
		"daisyui": "^4.0.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^9.0.0",
		"postcss": "^8.4.18",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.1",
		"publint": "^0.2.7",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.2.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.11",
		"vitest": "^1.3.0"
	},
	"type": "module",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts"
}
